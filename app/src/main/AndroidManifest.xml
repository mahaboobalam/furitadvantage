        <activity android:name="com.mahaboob.rum.mousumifruit.Main2Activity"/>
        <activity android:name="com.mahaboob.rum.mousumifruit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
